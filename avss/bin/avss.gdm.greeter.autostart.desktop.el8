[Desktop Entry]
Name=AVSS Start
Comment=AVSS Stage-1 Start Script
Exec=bash -c "export DISPLAY=:0.0;xhost +;export AVSS_ROOT=/opt/avss;$AVSS_ROOT/bin/avss.source.start.command.sh :0.0 1>> /tmp/avss.source.start.$USER.out 2>> /tmp/avss.source.start.$USER.err &"
